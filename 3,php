
<?php


$DUMBWAYS = array ('D','U','M','B','W','A','Y','S');

function drawLine($DUMBWAYS)
{


for ($a = 0; $a <=7; $a++) {
    for ($b = 1; $b <= $a; $b++) {
        echo "&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;";

    }
    for ($c = 1; $c <= 1; $c++) {
        echo "$DUMBWAYS[$a]";
        echo "<br>";
    }
    echo "<br>";
}

}
drawLine($DUMBWAYS);


?>
